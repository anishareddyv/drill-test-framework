{
    "testId": "parquet-tpch-parquet-stats-data",
    "type": "group",
    "description": "Test tpch queries with table stats via jdbc",
    "categories": [
        "smoke"
    ],
    "matrices": [
        {
            "query-file": ".*.q",
            "schema": "dfs.drillTestDir",
            "output-format": "tsv",
            "expected-file": ".*.e_tsv",
            "verification-type": [
                "in-memory"
            ]
        }
    ],
    "datasources": [
        {
            "mode": "cp",
            "src": "Datasources/Tpch0.01/parquet/customer.parquet",
            "dest": "table_stats/Tpch0.01/parquet/customer/customer.parquet"
        },
        {
            "mode": "cp",
            "src": "Datasources/Tpch0.01/parquet/lineitem.parquet",
            "dest": "table_stats/Tpch0.01/parquet/lineitem/lineitem.parquet"
        },
        {
            "mode": "cp",
            "src": "Datasources/Tpch0.01/parquet/nation.parquet",
            "dest": "table_stats/Tpch0.01/parquet/nation/nation.parquet"
        },
        {
            "mode": "cp",
            "src": "Datasources/Tpch0.01/parquet/region.parquet",
            "dest": "table_stats/Tpch0.01/parquet/region/region.parquet"
        },
        {
            "mode": "cp",
            "src": "Datasources/Tpch0.01/parquet/part.parquet",
            "dest": "table_stats/Tpch0.01/parquet/part/part.parquet"
        },
        {
            "mode": "cp",
            "src": "Datasources/Tpch0.01/parquet/partsupp.parquet",
            "dest": "table_stats/Tpch0.01/parquet/partsupp/partsupp.parquet"
        },
        {
            "mode": "cp",
            "src": "Datasources/Tpch0.01/parquet/supplier.parquet",
            "dest": "table_stats/Tpch0.01/parquet/supplier/supplier.parquet"
        },
        {
            "mode": "cp",
            "src": "Datasources/Tpch0.01/parquet/orders.parquet",
            "dest": "table_stats/Tpch0.01/parquet/orders/orders.parquet"
        },
        {
            "mode": "gen",
            "src": "Datasources/table_stats/table_stats_tpch_orig.sh",
            "dest": ""
        }
    ]
}
